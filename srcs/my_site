server {
	listen 80;
	listen [::]:80;

	server_name	my_site;

	listen 443 ssl;
	listen [::]:443;
	server_name         www.my_site.com;
    ssl_certificate     www.my_site.com.crt;
    ssl_certificate_key www.my_site.com.key;
    ssl_protocols       TLSv1 TLSv1.1 TLSv1.2;
    ssl_ciphers         HIGH:!aNULL:!MD5;
	
	root /data/www/;

	index index.html index.htm index.nginx-debian.html;

	location / {
		root data/www/;
	}
}
